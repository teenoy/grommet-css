{
  "name": "buddyboost-css",
  "version": "1.5.0",
  "description": "Standalone css for grommet ui library.",
  "main": "build/index.min.css",
  "scripts": {
    "build": "node-sass index.scss build/index.min.css --output-style compressed --include-path ./node_modules",
    "build:buddyboost": "node-sass themes/buddyboost/index.scss build/buddyboost/index.min.css --output-style compressed --include-path ./node_modules",
    "start": "node-sass index.scss build/index.min.css -w --output-style compressed --include-path ./node_modules"
  },
  "keywords": [
    "create-react-app",
    "grommet"
  ],
  "author": "Mel van Londen",
  "license": "MIT",
  "devDependencies": {
    "node-sass": "^4.5.1"
  },
  "dependencies": {
    "grommet": "^1.5.0"
  }
}
